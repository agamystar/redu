put type=\"button\" class=\"btn btn-primary\" value=\"Generate\"  @click=\"getData\"/>\n          </div>\n        </div>\n      </div>\n    </div>\n\n\n  <div class=\"row\" v-if=\"data_loaded\">\n    <div class=\"col-sm-12\">\n      <div class=\"form-group \">\n        <table class=\"table table-striped\">\n          <tr v-for=\"(row,index) in rows\">\n            <td>{{index+1}}</td>\n            <td>{{row}}</td>\n          </tr>\n        </table>\n      </div>\n    </div>\n  </div>\n\n</template> \n<style>\n@import 'https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css';\n </style>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data(){\n    return {num_pin_code:1 , data_loaded:false , rows:[] , counter:0}\n  },\n  methods:{\n      getData(){\n        return axios({\n          method:'get',\n          url:'http://127.0.0.1:8002/gen_pin?num_pin_code='+this.num_pin_code,\n          data:this.num_pin_code\n        })\n            .then(response => {\n               this.rows=response.data.data;\n               this.data_loaded=true;\n            }).catch(error => {\n              alert(\"Please set the right url in App.vue file\");\n              return error.response;\n            })  .finally(() => this.loading = false);\n\n      }\n  }\n}\n\n</script>"]}]}