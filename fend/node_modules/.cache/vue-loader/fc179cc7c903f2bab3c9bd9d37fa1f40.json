\n    export default { \n       data(){\n          return {\n               title:'Roles',\n               data:{},\n               rows:[],\n               columns:[]\n            }\n        },            \n       async created() {//run before template\n              if(this.$route.params.id>0){\n               this.edit(); \n              }\n              this.getPermissions();\n        },\n        methods:{\n            async store(){\n                 const res=await this.callApi(\"POST\",\"roles\",{\n                     data:this.data\n                 });\n                //this.$router.go(-1);\n                this.$router.back();\n            }, \n            async edit(){\n                 const res=await this.callApi(\"GET\",\"roles/\"+this.$route.params.id,{});\n                 this.data=res.data;\n                 //alert(JSON.stringify(res.data));\n            },\n            async getPermissions(){\n                 const res=await this.callApi(\"GET\",\"permissions?get_data=true\",{});\n                 this.data=res.data;\n                 alert(JSON.stringify(res.data));\n            },  \n             async update(){\n                 const res=await this.callApi(\"patch\",\"roles/\"+this.$route.params.id,{\n                     name:this.data.name\n                 });\n               this.$router.go(-1);\n            }\n           \n        }\n        \n    }\n</script>\n"]}]}