/vue-front/src/components/posts/Index.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"home\" ref=\"userref\">\n       <h4> {{title}}  </h4>\n       <div class=\"text-right\">        \n        <button style=\"display:none;\" @click=\"create();\" class=\"btn btn-primary\"> Add + </button>\n        <router-link to='posts/create' class=\"btn btn-primary\"> Add + </router-link>\n       </div>\n       <div>\n              <table class=\"table\" v-if=\"this.rows\">\n                <thead>\n                     <th v-for=\"col in this.columns\" :key=\"col.id\" >{{col.name}}</th>                     \n                </thead>\n                <tr v-for=\"row in this.rows\" :key=\"row.id\" >\n                    <td v-for=\"col in columns\" :key=\"col.id\" >\n                       <span v-if=\"col.id!='actions'\">{{row[col.id]}}</span>\n                       <span v-else>\n                              <span v-for=\"a in row[col.id] \" :key=\"a['name']\">                                 \n                                   <router-link v-if=\"a['name']!='Delete' \" :to='a[\"link\"]' :class='a[\"class\"]'>{{a[\"name\"]}}</router-link> \n                                   <button v-else  @click=\"deleteRow(row['id'])\" :class=\"a['class']\">{{a[\"name\"]}}</button> \n                              </span> &nbsp;\n                             \n                         </span>\n                     </td> \n                </tr>\n            </table>\n            <h3 v-else> There is no data .... </h3>\n       </div>\n      \n      \n      <!-- Modal -->\n<div class=\"modal fade\" id=\"modal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLabel\">Modal title</h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;</span>\n        </button>\n      </div>\n      <div class=\"modal-body\">\n       <div class=\"form-group\">\n           <div class=\"col-sm-12\">\n               <label for=\"\" class=\"form-label\"> Name : </label>\n               <input type=\"text\" class=\"form-control\" v-model=\"data.name\" />\n           </div>\n       </div>\n      </div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\" >Close</button>\n        <button type=\"button\" class=\"btn btn-primary\" @click=\"store();\">Save</button>\n      </div>\n    </div>\n  </div>\n</div>\n\n\n    </div>\n</template>\n \n<script>\n\n    export default { \n       data(){\n          return {\n               title:'posts',\n               data:{},\n               rows:[],\n               columns:[]\n            }\n        },            \n       async created() {//run before template\n               this.loadData();\n        },\n        methods:{\n            async loadData(){\n               const res=await this.callApi(\"get\",\"posts?get_data=true\",{});\n               this.rows= res.data.result.rows;\n               console.log(this.rows);\n               this.$store.commit('changeStatueLanguage','ar');\n               console.log(this.$store.getters.filterState);\n               this.columns=res.data.result.columns;\n            },\n            create(){\n                $(\"#modal\").modal(\"show\");\n            },\n            async store(){\n                 const res=await this.callApi(\"POST\",\"posts\",{\n                     data:this.data\n                 });\n               this.loadData();\n            },  \n           async deleteRow(id){\n                 const res=await this.callApi(\"DELETE\",\"posts/\"+id,{\n                 });\n                   this.loadData();\n            }\n        }\n        \n    }\n</script>\n"]}]}