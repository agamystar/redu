                 //alert(JSON.stringify(res.data));\n            },\n            async getPermissions(){\n                 const res=await this.callApi(\"GET\",\"permissions?get_data=true\",{});\n                 this.data=res.data;\n                 alert(JSON.stringify(res.data));\n            },  \n             async update(){\n                 const res=await this.callApi(\"patch\",\"roles/\"+this.$route.params.id,{\n                     name:this.data.name\n                 });\n               this.$router.go(-1);\n            }\n           \n        }\n        \n    }\n</script>\n"]}]}